{
  ".source.hydra": {
    "for in loop": {
      "prefix": "for",
      "body": "for ${1:var} in ${2:container} do\n\t${3://your code...}\nend"
    },
    "while loop": {
      "prefix": "whi",
      "body": "while ${1:condition} do\n\t${2://your code...}\nend"
    },
    "class": {
      "prefix": "cla",
      "body": "class ${1:Classname} ${2:extends} ${3:Superclass}\n\t${4:Classname}(${5:params}){\n\t${6://initialization...}\n\t}\nend"
    },
    "function": {
      "prefix": "fun",
      "body": "function ${1:name} (${2:params}) {\n\t${3://your code...}\n}"
    },
    "gen function": {
      "prefix": "gen",
      "body": "gen function ${1:name} (${2:params}) {\n\t${3://your code...}\n}"
    }
  }
}
